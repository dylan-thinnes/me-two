<%= render partial: "shared/section_colour" %>
<% set_title("Projects") %>
<%= to_markdown "## projects" %>
<% Project.all.each do |p| %>
    <p><a href="<%= p.link %>" class="entry">
      <b class="text"><%= p.name %></b><br/>
      <small>
        <%= p.content %>
      </small>
    </a></p>
<% end %>
